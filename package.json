{
  "name": "sapphire-template",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install .github/husky",
    "build": "rimraf tsconfig.tsbuildinfo dist && yarn tsc --listEmittedFiles --incremental",
    "build:watch": "yarn tsc -w --incremental",
    "lint": "eslint src/ --ext .ts --cache",
    "lint:fix": "eslint src/ --fix --ext .ts --cache",
    "start": "yarn node --enable-source-maps dist/main.js"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "@prisma/client": "^4.13.0",
    "@sapphire/decorators": "^6.0.1",
    "@sapphire/framework": "4.4.3",
    "@sapphire/pieces": "^3.6.3",
    "@sapphire/plugin-i18next": "^7.1.2",
    "@sapphire/shapeshift": "^3.8.2",
    "@sapphire/utilities": "^3.11.1",
    "colorette": "^2.0.19",
    "discord-api-types": "^0.37.41",
    "discord.js": "^14.10.2",
    "fs-extra": "^10.1.0",
    "i18next": "^22.4.15",
    "mysql2": "^2.3.3",
    "pino": "^8.4.2",
    "pino-pretty": "^9.1.0",
    "prisma": "^4.13.0",
    "ts-dotenv": "^0.8.3"
  },
  "devDependencies": {
    "@bitomic/eslint-config": "^1.0.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.56",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.2.3"
}
